import csv
import sorter
import os
import tkinter
from enum import Enum


####### USER INPUT #######
# File containing raw data
CSV_FILE = open('/Users/zack/Desktop/EXPORT.CSV', 'r')
# Runsheet date
DAY_OF_OPERATIONS = "2/18/2016"
##########################

# Shift sorter
s = sorter.sorter(CSV_FILE)

# Create and sort shifts list
shifts = s.createShifts(DAY_OF_OPERATIONS)

# Sort shifts list
shifts = s.sort(shifts)
for i in shifts:
    print(i)